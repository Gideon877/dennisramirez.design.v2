{
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": ["prettier",  "react"],
  "extends": ["eslint:recommended", "plugin:react/recommended","plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
